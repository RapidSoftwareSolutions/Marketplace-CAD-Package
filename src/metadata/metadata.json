{
    "package": "CAD",
    "tagline": "View, edit and get manufacturing prices for CAD designs.",
    "description": "Connect to the CAD.ai Manufacturing API to view, edit and get manufacturing prices for CAD designs. Test an API call and export the code snippet into your app.",
    "image": "https://logo.clearbit.com/cad.ai?s=128",
    "repo": "https://github.com/RapidSoftwareSolutions/Marketplace-CAD-Package",
    "keywords": [
        "API",
        "CAD",
        "CAM",
        "CAD.ai"
    ],
    "accounts": {
        "domain": "cad.ai",
        "credentials": [
            "apiKey"
        ]
    },
    "blocks": [
        {
            "name": "fixCADFiles",
            "description": "Fix your CAD files and get them ready for additive or subtractive manufacturing.",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                },
                {
                    "name": "fileUrl",
                    "type": "String",
                    "info": "Flle url for fix.",
                    "required": true
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        },
        {
            "name": "getCADModelParameters",
            "description": "Get information about 3D file, like boxarea, volume, xCenter, yCenter, zCenter.",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                },
                {
                    "name": "fileUrl",
                    "type": "String",
                    "info": "Flle url for fix.",
                    "required": true
                },
                {
                    "name": "unit",
                    "type": "String",
                    "info": "Unit of the design. One of: mm, in.",
                    "required": true
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        },
        {
            "name": "editCADModel",
            "description": "Allows you to rotate, mirror, scale your model.",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                },
                {
                    "name": "fileUrl",
                    "type": "String",
                    "info": "Flle url for transform.",
                    "required": true
                },
                {
                    "name": "scaleFactor",
                    "type": "String",
                    "info": "Scale factor.",
                    "required": true
                },
                {
                    "name": "xyMirror",
                    "type": "Boolean",
                    "info": "Mirror the mesh about XY plane.",
                    "required": true
                },
                {
                    "name": "yzMirror",
                    "type": "Boolean",
                    "info": "Mirror the mesh about YZ plane.",
                    "required": true
                },
                {
                    "name": "xzMirror",
                    "type": "Boolean",
                    "info": "Mirror the mesh about XZ plane.",
                    "required": true
                },
                {
                    "name": "xRotateAngle",
                    "type": "Number",
                    "info": "Angle to rotate the entire mesh about the x axis by the given number of degrees.",
                    "required": false
                },
                {
                    "name": "yRotateAngle",
                    "type": "Number",
                    "info": "Angle to rotate the entire mesh about the y axis by the given number of degrees.",
                    "required": false
                },
                {
                    "name": "zRotateAngle",
                    "type": "Number",
                    "info": "Angle to rotate the entire mesh about the z axis by the given number of degrees.",
                    "required": false
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        },
        {
            "name": "generateThumbnailFromCADFile",
            "description": "Generate thumbnail images of any sizes from a given 3D file.",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                },
                {
                    "name": "fileUrl",
                    "type": "String",
                    "info": "Flle url for fix.",
                    "required": true
                },
                {
                    "name": "width",
                    "type": "Number",
                    "info": "Width of the image.",
                    "required": true
                },
                {
                    "name": "height",
                    "type": "Number",
                    "info": "Height of the image.",
                    "required": true
                },
                {
                    "name": "xRotateAngle",
                    "type": "Number",
                    "info": "Angle to rotate the entire mesh about the x axis by the given number of degrees.",
                    "required": false
                },
                {
                    "name": "yRotateAngle",
                    "type": "Number",
                    "info": "Angle to rotate the entire mesh about the y axis by the given number of degrees.",
                    "required": false
                },
                {
                    "name": "zRotateAngle",
                    "type": "Number",
                    "info": "Angle to rotate the entire mesh about the z axis by the given number of degrees.",
                    "required": false
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        },
        {
            "name": "convertCADFile",
            "description": "Convert any of the 51 supported formats into .stl, .obj, .dae or .ply formats. Supported formats: .stl, .obj, .3ds, .ac, .ac3d, .acc, .ase, .ask, .b3d, .blend, .bvh, .cob, .csm, .dae, .dxf, .enff, .hmp, .ifc, .irr, .irrmesh, .lwo,.lws, .lxo, .m3, .md2, .md3, .md5anim, .md5camera, .md5mesh, .mdc, .mdl, .mesh.xml, .mot, .ms3d, .ndo, .nff, .obj, .off, .pk3, .ply, .prj, .q3o, .q3s, .raw, .scn, .smd, .vta, .x, .xgl, .xml, .zgl",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                },
                {
                    "name": "fileUrl",
                    "type": "String",
                    "info": "Flle url for fix.",
                    "required": true
                },
                {
                    "name": "fileFormat",
                    "type": "String",
                    "info": "File format to convert file into. One of: .stl, .dae, .ply, .obj.",
                    "required": true
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        },
        {
            "name": "get3DPrintingMaterials",
            "description": "Return list of materials types.",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        },
        {
            "name": "get3DModelPrintPrice",
            "description": "Query the price that would be billed to 3D print a given design.",
            "args": [
                {
                    "name": "apiKey",
                    "type": "credentials",
                    "info": "Your Public key.",
                    "required": true
                },
                {
                    "name": "fileUrl",
                    "type": "String",
                    "info": "Flle url for fix.",
                    "required": true
                },
                {
                    "name": "material",
                    "type": "String",
                    "info": "One of material types.",
                    "required": true
                },
                {
                    "name": "unit",
                    "type": "String",
                    "info": "Unit of the design. One of: mm, in.",
                    "required": false
                },
                {
                    "name": "currency",
                    "type": "String",
                    "info": "Currency in which price is returned. One of: EUR, USD.",
                    "required": false
                }
            ],
            "callbacks": [
                {
                    "name": "error",
                    "info": "Error"
                },
                {
                    "name": "success",
                    "info": "Success"
                }
            ]
        }
    ]
}